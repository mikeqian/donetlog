<?xml version="1.0" encoding="windows-1250" ?>
<configuration>
    <configSections>
      <section name="mLog" type="Framework.Qlh.Log.Config.ConfigSectionHandler, Framework.Qlh.Log" />
    </configSections>
  <mLog autoReload="true">
        <appenders>
            <appender name="console" type="Console" layout="CONFIG1: ${machinename}:${message}" />
        </appenders>
        <rules>
            <logger name="*" level="Debug" appendTo="console" />
        </rules>
    </mLog>
</configuration>
